#include <iostream>
using namespace std;
int main()
{
    int a, b, q;
    cout << "введите значения а и б" << endl;
    cin >> q;       // вводим значения
    cin >> b;

    for (int a = q; a <= b; a++) {               //вцикл выводит значения от а до б

        for (int i = 0; i < a; i++) {           //цикл выводит значения а столько раз, чему равно а
            cout << a << "";

        }
        cout << endl;
    }
    return 0;
}
